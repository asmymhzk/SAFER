<div class="intro intro-carousel swiper position-relative">

    <div class="swiper-wrapper">

    {% for property in properties %}
    
      <div class="swiper-slide carousel-item-a intro-item bg-image" style
      ="background-image: url(assets/img/slide-1.jpg)">
            <div class="overlay overlay-a"></div>
                <div class="intro-content display-table">
                <div class="table-cell">
                    <div class="container">
                        <div class="row">
                            <div class="col-lg-8">
                                <div class="intro-body">
                                    <p class="intro-title-top"> {{property.ville}}
                                    <br> {{property.CodePostal}}
                                    </p>
                                    <h1 class="intro-title mb-4 ">
                                        <span class="color-b">
                                        {{property.id}} </span>
                                         {{property.CategorieType}}
                                        <br> {{property.titre}}
                                    </h1>
                                    <p class="intro-subtitle intro-price">
                                        <a href="#"><span class="price-a">
                                     {{property.StatusType}} | {{property.formattedPrix}} â‚¬</span></a>
                                    </p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    {% endfor %}    
    </div>
    <div class="swiper-pagination"></div>
  </div>
